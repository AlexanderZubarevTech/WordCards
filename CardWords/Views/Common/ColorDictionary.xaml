    <SolidColorBrush x:Key="WordResultTextBrush" Color="{StaticResource WordResultTextColor}"></SolidColorBrush>



    <Color x:Key="ValidationErrorFieldColor" A="0xFF" R="0x6C" G="0x24" B="0x21"/>
    <SolidColorBrush x:Key="ValidationErrorFieldBrush" Color="{StaticResource ValidationErrorFieldColor}"></SolidColorBrush>

    <Color x:Key="ValidationErrorTextColor" A="0xFF" R="0x6C" G="0x24" B="0x21"/>
    <SolidColorBrush x:Key="ValidationErrorTextBrush" Color="{StaticResource ValidationErrorTextColor}"></SolidColorBrush>
    
</ResourceDictionary>