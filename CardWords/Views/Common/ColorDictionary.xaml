    <Color x:Key="DefaultActiveColor" A="0xFF" R="0x4E" G="0x32" B="0x82"/>
    <SolidColorBrush x:Key="DefaultActiveBrush" Color="{StaticResource DefaultActiveColor}"></SolidColorBrush>

    <Color x:Key="DefaultActiveTextColor" A="0xFF" R="0xCC" G="0xCC" B="0xCC"/>
    <SolidColorBrush x:Key="DefaultActiveTextBrush" Color="{StaticResource DefaultActiveTextColor}"></SolidColorBrush>



    <Color x:Key="HoverColor" A="0xFF" R="0x00" G="0x97" B="0xD3"/>
    <SolidColorBrush x:Key="HoverBrush" Color="{StaticResource HoverColor}"></SolidColorBrush>

