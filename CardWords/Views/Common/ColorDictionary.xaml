    <SolidColorBrush x:Key="HoverActiveTextBrush" Color="{StaticResource HoverActiveTextColor}"></SolidColorBrush>



    <Color x:Key="CorrectColor" A="0xFF" R="0x03" G="0x84" B="0x24"/>
    <SolidColorBrush x:Key="CorrectBrush" Color="{StaticResource CorrectColor}"></SolidColorBrush>

    <Color x:Key="CorrectLineColor" A="0xFF" R="0x35" G="0xA2" B="0x51"/>
