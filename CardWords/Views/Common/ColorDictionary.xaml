<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="DefaultBackgroundColor" A="0xFF" R="0xE9" G="0xE9" B="0xE9"/>
    <SolidColorBrush x:Key="DefaultBacgroundBrush" Color="#e9e9e9"></SolidColorBrush>
    
    

    <Color x:Key="DefaultColor" A="0xFF" R="0x00" G="0x79" B="0xA9"/>
    <SolidColorBrush x:Key="DefaultBrush" Color="{StaticResource DefaultColor}"></SolidColorBrush>

    <Color x:Key="DefaultLineColor" A="0xFF" R="0x00" G="0x97" B="0xD3"/>
    <SolidColorBrush x:Key="DefaultLineBrush" Color="{StaticResource DefaultLineColor}"></SolidColorBrush>

    <Color x:Key="DefaultTextColor" A="0xFF" R="0xDD" G="0xDD" B="0xDD"/>
    <SolidColorBrush x:Key="DefaultTextBrush" Color="{StaticResource DefaultTextColor}"></SolidColorBrush>



    <Color x:Key="NotEnabledButtonColor" A="0xFF" R="0x88" G="0x88" B="0x88"/>
    <SolidColorBrush x:Key="NotEnabledButtonBrush" Color="{StaticResource NotEnabledButtonColor}"></SolidColorBrush>

    <Color x:Key="NotEnabledButtonTextColor" A="0xFF" R="0x66" G="0x66" B="0x66"/>
    <SolidColorBrush x:Key="NotEnabledButtonTextBrush" Color="{StaticResource NotEnabledButtonTextColor}"></SolidColorBrush>

    

    <Color x:Key="CancelButtonColor" A="0xFF" R="0xC0" G="0xC0" B="0xC0"/>
